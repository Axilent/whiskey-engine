#!/usr/bin/env bash

# Sets environment variables for local execution of Axilent Docs
export AXILENT_DOCS_DEBUG=true
export AXILENT_DOCS_SECRET_DJANGO_KEY='<your-django-secret-key>'
export AXILENT_DOCS_API_KEY = '<your-axilent-api-key>'
export AWS_ACCESS_KEY_ID='<your-aws-access-key-id>'
export AWS_SECRET_ACCESS_KEY='<your-aws-secret-access-key>'

# Run
./manage.py $1 $2 $3

